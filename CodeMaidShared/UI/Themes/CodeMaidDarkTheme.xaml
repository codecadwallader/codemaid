<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="CodeMaidCoreTheme.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="FWindow" Color="#FF252526" />
    <SolidColorBrush x:Key="FGWindow" Color="#FFF1F1F1" />

    <SolidColorBrush x:Key="FOverlay" Color="#80252526" />

    <SolidColorBrush x:Key="FItemParent" Color="#FF252E3C" />
    <SolidColorBrush x:Key="FItemHighlight" Color="#FF253B3C" />

    <SolidColorBrush x:Key="BGItemHighlight" Color="#FFFEFCC8" />
    <SolidColorBrush x:Key="FGItemHighlight" Color="#FF1E1E1E" />
    <SolidColorBrush x:Key="FGType" Color="#FF777777" />

    <ImageSource x:Key="ImageCollapsed">/SteveCadwallader.CodeMaid;component/UI/Themes/Images/CollapsedDark.png</ImageSource>
    <ImageSource x:Key="ImageExpanded">/SteveCadwallader.CodeMaid;component/UI/Themes/Images/ExpandedDark.png</ImageSource>
</ResourceDictionary>