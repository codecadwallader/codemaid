<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="CodeMaidCoreTheme.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="FWindow" Color="#FFF6F6F6" />
    <SolidColorBrush x:Key="FGWindow" Color="#FF1E1E1E" />

    <SolidColorBrush x:Key="FOverlay" Color="#80F6F6F6" />

    <SolidColorBrush x:Key="FItemParent" Color="#FFE0EDF6" />
    <SolidColorBrush x:Key="FItemHighlight" Color="#FFE0F6F3" />

    <SolidColorBrush x:Key="BGItemHighlight" Color="#FFD0F7FF" />
    <SolidColorBrush x:Key="FGItemHighlight" Color="#FF1E1E1E" />
    <SolidColorBrush x:Key="FGType" Color="#FF777777" />

    <ImageSource x:Key="ImageCollapsed">/SteveCadwallader.CodeMaid;component/UI/Themes/Images/CollapsedLight.png</ImageSource>
    <ImageSource x:Key="ImageExpanded">/SteveCadwallader.CodeMaid;component/UI/Themes/Images/ExpandedLight.png</ImageSource>
</ResourceDictionary>